<view class="app">
    <view class="ele-breakline"></view>
    <view style="text-align:center;">
      <image class="login_avatar" src="{{userInfo.avatarUrl}}"></image>
    </view>
    <view class="element breakline"style="border-width:1px;border-bottom-style:solid;margin-top:11px;margin-left:auto;margin-right;:auto;width:375px;border-bottom-color:rgb(255, 153, 0);"></view>
    <view class="table">
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/self_info.png"></image>
                </view>
                <view class="td text_td" >
                    <text>个人信息</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="wallet">
                        <image class="image_icon_next"  src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/my_plan.png"></image>
                </view>
                <view class="td text_td" >
                    <text>我的方案</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="health_data">
                        <image class="image_icon_next" src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/wallet.png"></image>
                </view>
                <view class="td text_td" >
                    <text>钱包</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="charge_standard">
                        <image class="image_icon_next" src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
        
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/team_clock.png"></image>
                </view>
                <view class="td text_td" >
                    <text>团队打卡</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="hotline">
                        <image class="image_icon_next" src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/service.png"></image>
                </view>
                <view class="td text_td" >
                    <text>联系客服</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="hotline">
                        <image class="image_icon_next" src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
        <block>
            <view class = "tr">
                <view class="td icon_td" >
                    <image class="image_icon"  src="../../images/about_us.png"></image>
                </view>
                <view class="td text_td" >
                    <text>关于我们</text>
                </view>
                <view class="td next_td" >
                    <view bindtap="hotline">
                        <image class="image_icon_next" src="../../images/next_page.png"></image>
                    </view>
                </view>
            </view>
        </block>
    </view>
</view>